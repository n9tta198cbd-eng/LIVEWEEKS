{
  "functions": {
    "api/generate.py": {
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|otf|ttf|woff|woff2))",
      "destination": "/src/$1"
    },
    {
      "source": "/(img|fonts)/(.*)",
      "destination": "/src/$1/$2"
    }
  ]
}
