{
  "functions": {
    "api/generate.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/generate",
      "destination": "/api/generate.py"
    }
  ]
}
